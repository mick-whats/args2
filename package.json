{
  "name": "args2",
  "version": "1.2.3",
  "description": "You can easily handle arguments of your own function.",
  "scripts": {
    "test": "ava",
    "watch": "ava --watch",
    "cover": "nyc ava",
    "report": "nyc report --reporter=html",
    "update:snap": "ava --update-snapshots"
  },
  "keywords": [
    "arguments"
  ],
  "dependencies": {
    "lodash": "^4.7.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mick-whats/args2-node"
  },
  "coffeelintConfig": {
    "max_line_length": {
      "level": "ignore"
    }
  },
  "ava": {
    "files": [
      "test/*.test.js"
    ],
    "source": [
      "**/*.js"
    ],
    "failFast": false,
    "serial": false,
    "tap": false,
    "verbose": true,
    "color": true,
    "watch": false
  },
  "main": "lib/index.js",
  "license": "MIT",
  "author": "mick-whats"
}
